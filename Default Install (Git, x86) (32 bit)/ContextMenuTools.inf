HKCR,Drive\Shell\StartPowerShellHereAdministratorExtended,Icon,0x00020000,"%11%\PowerShellAdmin.ico"
HKCR,Drive\Shell\StartPowerShellHereAdministratorExtended,Extended,,""

HKCR,Drive\Shell\Git_Shell,,,"%GitContextText%"
HKCR,Drive\Shell\Git_Shell\command,,,"wscript ""%24%\Program Files\Git\Git Bash.vbs"" ""%1"""
HKCR,Drive\Shell\Git_Shell,Icon,0x00020000,"%11%\Git.ico"

HKCR,Drive\Shell\CommandPromptHereAdministratorExtended,,,"%cmdContextText%"
HKCR,Drive\Shell\CommandPromptHereAdministratorExtended,Icon,0x00020000,"%11%\cmdAdmin.ico"
HKCR,Drive\Shell\CommandPromptHereAdministratorExtended,Extended,,""
HKCR,Drive\Shell\CommandPromptHereAdministratorExtended\command,,0x00020000,"""%11%\cmd.exe"" /c Elevate.cmd ""%11%\cmd.exe"" /s /k ""COLOR 0C && VER && PUSHD ""%1"""""

