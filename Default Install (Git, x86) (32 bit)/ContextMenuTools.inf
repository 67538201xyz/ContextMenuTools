[version]
Signature="$Windows NT$"

[ContextMenuToolsInstall]
CopyFiles = ContextMenuTools.Files.Inf
CopyFiles = ContextMenuTools.Files.System
CopyFiles = ContextMenuTools.Files.Icon
AddReg    = ContextMenuTools.AddReg
DelReg    = cmd.DelReg

[DefaultInstall]
CopyFiles = ContextMenuTools.Files.Inf
CopyFiles = ContextMenuTools.Files.System
CopyFiles = ContextMenuTools.Files.Icon
AddReg    = ContextMenuTools.AddReg
DelReg    = cmd.DelReg

[DefaultUnInstall]
DelFiles  = ContextMenuTools.Files.Inf
DelReg    = ContextMenuTools.DelReg
AddReg    = cmd.AddReg

[SourceDisksNames]
55 = "Context Menu Tools","",1

[SourceDisksFiles]
ContextMenuTools.INF = 55
cmd.ico              = 55
cmdAdmin.ico         = 55
PowerShell.ico       = 55
PowerShellAdmin.ico  = 55
Git.ico              = 55

[DestinationDirs]
ContextMenuTools.Files.Inf    = 17
ContextMenuTools.Files.System = 11
ContextMenuTools.Files.Icon   = 11

[ContextMenuTools.Files.Inf]
ContextMenuTools.INF

[ContextMenuTools.Files.System]
Elevate.cmd
Elevate.vbs

[ContextMenuTools.Files.Icon]
cmd.ico
cmdAdmin.ico
PowerShell.ico
PowerShellAdmin.ico
Git.ico

[ContextMenuTools.AddReg]
HKLM,%UDHERE%,DisplayName,,"%DisplayName%"
HKLM,%UDHERE%,DisplayVersion,,"%DisplayVersion%"
HKLM,%UDHERE%,DisplayIcon,,"%11%\PowerShellAdmin.ico"
HKLM,%UDHERE%,UninstallString,,"rundll32.exe syssetup.dll,SetupInfObjectInstallAction DefaultUninstall 132 %17%\ContextMenuTools.inf"

;Adds entries when right-clicking a Directory
HKCR,Directory\Shell\StartPowerShellHere,,,"%PowerShellContextText%"
HKCR,Directory\Shell\StartPowerShellHere\command,,,"""%11%\windowspowershell\v1.0\PowerShell.exe"" -NoExit -Command ""Set-Location -LiteralPath '%L'; $global:CurrentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent(); function Prompt { Write-Host ('' + $CurrentUser.Name) -nonewline -foregroundcolor DarkGreen; Write-Host (' ' + $(get-location) +'>') -nonewline; return ' ' }; """
HKCR,Directory\Shell\StartPowerShellHere,Icon,0x00020000,"%11%\PowerShell.ico"

HKCR,Directory\Shell\StartPowerShellHereAdministratorExtended,,,"%PowerShellAdminContextText%"
HKCR,Directory\Shell\StartPowerShellHereAdministratorExtended\command,,0x00020000,"""%SystemRoot%\System32\cmd.exe"" /c Elevate.cmd ""%SystemRoot%\System32\windowspowershell\v1.0\powershell.exe"" -NoExit -Command ""Set-Location -LiteralPath '%L'; if (!$host.ui.rawui.windowtitle.Contains('Administrator')) { $host.ui.rawui.windowtitle = 'Administrator: ' + $host.ui.rawui.windowtitle }; $global:CurrentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent(); function Prompt { Write-Host ('Administrator') -nonewline -foregroundcolor Red; Write-Host (' ' + $(get-location) + '>') -nonewline; return ' '; }; """
HKCR,Directory\Shell\StartPowerShellHereAdministratorExtended,Icon,0x00020000,"%11%\PowerShellAdmin.ico"
HKCR,Directory\Shell\StartPowerShellHereAdministratorExtended,Extended,,""

HKCR,Directory\Shell\Git_Shell,,,"%GitContextText%"
HKCR,Directory\Shell\Git_Shell\command,,,"wscript ""%24%\Program Files\Git\Git Bash.vbs"" ""%1"""
HKCR,Directory\Shell\Git_Shell,Icon,0x00020000,"%11%\Git.ico"

HKCR,Directory\Shell\CommandPromptHereAdministratorExtended,,,"%cmdContextText%"
HKCR,Directory\Shell\CommandPromptHereAdministratorExtended,Icon,0x00020000,"%11%\cmdAdmin.ico"
HKCR,Directory\Shell\CommandPromptHereAdministratorExtended,Extended,,""
HKCR,Directory\Shell\CommandPromptHereAdministratorExtended\command,,0x00020000,"""%11%\cmd.exe"" /c Elevate.cmd ""%11%\cmd.exe"" /s /k ""COLOR 0C && VER && PUSHD ""%1"""""

;Adds entries when right-clicking "empty space" in a directory
HKCR,Directory\Background\Shell\StartPowerShellHere,,,"%PowerShellContextText%"
HKCR,Directory\Background\Shell\StartPowerShellHere\command,,,"""%11%\windowspowershell\v1.0\PowerShell.exe"" -NoExit -Command ""$global:CurrentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent(); function Prompt { Write-Host ('' + $CurrentUser.Name) -nonewline -foregroundcolor DarkGreen; Write-Host (' ' + $(get-location) +'>') -nonewline; return ' ' }; """
HKCR,Directory\Background\Shell\StartPowerShellHere,Icon,0x00020000,"%11%\PowerShell.ico"

HKCR,Directory\Background\Shell\StartPowerShellHereAdministratorExtended,,,"%PowerShellAdminContextText%"
HKCR,Directory\Background\Shell\StartPowerShellHereAdministratorExtended\command,,0x00020000,"""%SystemRoot%\System32\cmd.exe"" /c Elevate.cmd ""%SystemRoot%\System32\windowspowershell\v1.0\powershell.exe"" -NoExit -Command ""if (!$host.ui.rawui.windowtitle.Contains('Administrator')) { $host.ui.rawui.windowtitle = 'Administrator: ' + $host.ui.rawui.windowtitle }; $global:CurrentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent(); function Prompt { Write-Host ('Administrator') -nonewline -foregroundcolor Red; Write-Host (' ' + $(get-location) + '>') -nonewline; return ' '; }; """
HKCR,Directory\Background\Shell\StartPowerShellHereAdministratorExtended,Icon,0x00020000,"%11%\PowerShellAdmin.ico"
HKCR,Directory\Background\Shell\StartPowerShellHereAdministratorExtended,Extended,,""

HKCR,Directory\Background\Shell\Git_Shell,,,"%GitContextText%"
HKCR,Directory\Background\Shell\Git_Shell\command,,,"wscript ""%24%\Program Files\Git\Git Bash.vbs"""
HKCR,Directory\Background\Shell\Git_Shell,Icon,0x00020000,"%11%\Git.ico"

HKCR,Directory\Background\Shell\CommandPromptHereAdministratorExtended,,,"%cmdContextText%"
HKCR,Directory\Background\Shell\CommandPromptHereAdministratorExtended,Icon,0x00020000,"%11%\cmdAdmin.ico"
HKCR,Directory\Background\Shell\CommandPromptHereAdministratorExtended,Extended,,""
HKCR,Directory\Background\Shell\CommandPromptHereAdministratorExtended\command,,0x00020000,"""%11%\cmd.exe"" /c Elevate.cmd ""%11%\cmd.exe"" /s /k ""COLOR 0C && VER"""

;Adds entries when right-clicking a Drive
HKCR,Drive\Shell\StartPowerShellHere,,,"%PowerShellContextText%"
HKCR,Drive\Shell\StartPowerShellHere\command,,,"""%11%\windowspowershell\v1.0\PowerShell.exe"" -NoExit -Command ""Set-Location -LiteralPath '%L'; $global:CurrentUser = [System.Security.Principal.WindowsIdentity]::GetCurrent(); function Prompt { Write-Host ('' + $CurrentUser.Name) -nonewline -foregroundcolor DarkGreen; Write-Host (' ' + $(get-location) +'>') -nonewline; return ' ' }; """
